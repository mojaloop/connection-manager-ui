{
  "name": "connection-manager-ui",
  "version": "1.8.4",
  "private": true,
  "dependencies": {
    "@pm4ml/mojaloop-payment-manager-ui-components-legacy": "1.23.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-react-app": "^10.0.1",
    "connected-react-router": "^6.3.2",
    "fetch-mock": "^12.4.0",
    "gensync": "^1.0.0-beta.2",
    "history": "^4.9.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.25",
    "prettier": "^1.17.0",
    "qrcode": "^1.4.1",
    "react": "^16.8.5",
    "react-app-rewired": "^2.1.1",
    "react-dom": "^16.8.5",
    "react-redux": "^6.0.1",
    "react-router-dom": "^5.0.0",
    "react-scripts": "^5.0.1",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.5",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "sass": "^1.83.4",
    "sass-loader": "^7.3.1",
    "svg-sprite-loader": "^6.0.11"
  },
  "scripts": {
    "prettier": "prettier --write 'src/**/*.{css,js}'",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "audit:check": "npx audit-ci --config ./audit-ci.jsonc",
    "dep:check": "npx ncu -e 2",
    "dep:update": "npx ncu -u",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src --color",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx src --color --fix",
    "release": "standard-version --releaseCommitMessageFormat 'chore(release): {{currentTag}} [skip ci]'",
    "snapshot": "standard-version --no-verify --skip.changelog --prerelease snapshot --releaseCommitMessageFormat 'chore(snapshot): {{currentTag}}'",
    "test:xunit": "react-app-rewired test --watchAll",
    "test:nowatch": "CI=true react-app-rewired test --env=jsdom",
    "test:coverage": "CI=true npx nyc --reporter=lcov --reporter=text  --reporter=text-summary npm run test -- --coverage --watchAll=false",
    "test:coverage-check": "npm run test:coverage && nyc check-coverage"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "resolutions": {
    "node-sass": "npm:sass",
    "sass-loader": "^7.3.1",
    "webpack": "4.28.3"
  },
  "devDependencies": {
    "jest": "^27.5.1"
  }
}
