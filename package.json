  },
  "scripts": {
    "prettier": "prettier --write 'src/**/*.{css,js}'",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "build:prod": "cross-env GENERATE_SOURCEMAP=false NODE_ENV=production react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "audit:check": "npx audit-ci --config ./audit-ci.jsonc",
    "dep:check": "npx ncu -e 2",
    "dep:update": "npx ncu -u",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src --color",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx src --color --fix",
    "release": "standard-version --releaseCommitMessageFormat 'chore(release): {{currentTag}} [skip ci]'",
    "snapshot": "standard-version --no-verify --skip.changelog --prerelease snapshot --releaseCommitMessageFormat 'chore(snapshot): {{currentTag}}'",
    "test:xunit": "react-app-rewired test --watchAll=false",
    "test:nowatch": "CI=true react-app-rewired test --env=jsdom",
    "test:coverage": "CI=true npx nyc --reporter=lcov --reporter=text --reporter=text-summary --reporter=clover npm run test --watchAll=false -- --coverage --detectOpenHandles --forceExit",
    "test:coverage-check": "npm run test:coverage && npx nyc check-coverage"

  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "resolutions": {
    "node-sass": "npm:sass",
    "sass-loader": "^7.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@babel/plugin-transform-runtime": "^7.26.0",
    "@types/chance": "^1.1.6",
    "babel-jest": "^29.7.0",
    "dotenv": "^16.4.7",
    "eslint": "^7.32.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jest": "^27.5.1",
    "jest-environment-jsdom": "^27.5.1",
    "jest-environment-node": "^27.5.1",
    "jest-junit": "^16.0.0",
    "jsdom": "^16.7.0",
    "svg-sprite-loader": "^6.0.11",
    "testcafe": "^3.7.2",
    "ts-jest": "^27.1.5",
    "typescript": "^4.9.5"
  }
