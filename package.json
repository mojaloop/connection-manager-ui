    "moment": "^2.24.0",
    "moment-timezone": "^0.5.25",
    "node-sass": "^4.14.1",
    "prettier": "^1.17.0",
    "qrcode": "^1.4.1",
    "react": "^16.8.5",
    "react-app-rewired": "^2.1.1",
    "react-dom": "^16.8.5",
